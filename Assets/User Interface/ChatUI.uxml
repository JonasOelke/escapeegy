                <ui:VisualElement style="background-image: url(&apos;project://database/Assets/Pictures/Tante%20Emma.PNG?fileID=21300000&amp;guid=844f9f3cc9c57484e846f10649b1c3b7&amp;type=3#Tante Emma&apos;); width: 150px; height: 150px; padding-top: 0; padding-left: 0; padding-right: 0; margin-left: 30px; margin-right: 30px;" />
                <ui:Label text="Tante Emma" display-tooltip-when-elided="true" style="-unity-font: url(&apos;project://database/Assets/User%20Interface/Fonts/FleurDeLeah-Regular.ttf?fileID=12800000&amp;guid=ae0bb9a6c47c8d44ea6380f0505d038e&amp;type=3#FleurDeLeah-Regular&apos;); -unity-font-style: normal; font-size: 118px; -unity-font-definition: url(&apos;project://database/Assets/User%20Interface/Fonts/FleurDeLeah-Regular%20SDF.asset?fileID=11400000&amp;guid=77987df44ac04eb4882d36d8ac782442&amp;type=2#FleurDeLeah-Regular SDF&apos;); color: rgb(255, 232, 204); margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 20px; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0;" />
            </ui:VisualElement>
            <ui:ScrollView scroll-deceleration-rate="0,135" elasticity="0,1" name="ChatMessagesContainer" style="padding-left: 90px; padding-right: 90px; padding-top: 40px; padding-bottom: 36px; background-color: rgba(0, 0, 0, 0); position: relative; justify-content: flex-start; align-items: flex-start; flex-shrink: 0; height: 75%;">
                <ui:Label text="Hallo Hallo du, ich habe ein Foto gefunden, zu dem Papa meinte, dass du mehr wissen könntest. Ich interessiere mich für unsere Familiengeschichte, weil ich das für die Schule machen muss. Hast du noch mehr Infos dazu? Wer waren die beiden? Emma!" class="chatMessage chatMessageRight" style="padding-top: 70px; padding-bottom: 70px;" />
                <ui:VisualElement class="chatMessageLeftContainer">
                    <ui:Label text=" Mal gucken was passiert wen ich jeztzt immer mehr text da rein schreibe" display-tooltip-when-elided="true" class="chatMessageLeft" style="margin-right: 0; -unity-text-align: upper-left; align-items: flex-start; justify-content: flex-start; white-space: normal; -unity-background-scale-mode: scale-to-fit; width: auto; min-width: 0; min-height: 0; max-width: none; flex-wrap: wrap;" />
                </ui:VisualElement>
                <ui:VisualElement class="chatMessageRightContainer">
                    <ui:Label text="Label" display-tooltip-when-elided="true" class="chatMessage chatMessageRight" style="align-items: flex-end; justify-content: flex-start;" />
                </ui:VisualElement>
